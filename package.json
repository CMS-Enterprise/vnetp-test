{
  "name": "automation-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "analyze": "webpack-bundle-analyzer dist/automation-ui/stats-es2015.json",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "build:prod": "ng build --configuration production",
    "documentation": "compodoc --watch --serve --open",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --no-cache --runInBand",
    "test:ci": "jest --ci --no-cache --runInBand --coverage --verbose --silent",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "sonar": "sonar-scanner",
    "generate-client": "TS_POST_PROCESS_FILE=./node_modules/.bin/prettier openapi-generator-cli generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.17",
    "@angular/common": "^12.2.17",
    "@angular/compiler": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/forms": "^12.2.17",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@angular/router": "^12.2.17",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-common-types": "^0.2.29",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ng-select/ng-select": "^7.4.0",
    "@swimlane/ngx-charts": "^16.0.0",
    "bootstrap": "^4.5.0",
    "d3": "^5.9.2",
    "d3-context-menu": "^1.1.1",
    "d3-scale": "^3.0.0",
    "d3-shape": "^1.3.5",
    "jquery": "^3.7.0",
    "ngx-pagination": "^5.0.0",
    "ngx-papaparse": "^5.0.0",
    "ngx-smart-modal": "^7.1.1",
    "ngx-toastr": "^12.1.0",
    "rxjs": "~6.5.3",
    "save-svg-as-png": "^1.4.14",
    "tslib": "^2.0.0",
    "validator": "^13.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.17",
    "@angular/language-service": "^12.2.17",
    "@compodoc/compodoc": "^1.1.11",
    "@openapitools/openapi-generator-cli": "^2.3.3",
    "@types/d3": "^5.7.2",
    "@types/jest": "^26.0.9",
    "@types/ldapjs": "0.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "codelyzer": "^6.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^3.1.0",
    "jest": "^26.3.0",
    "jest-preset-angular": "8.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.8",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "^26.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && lint-staged"
    }
  },
  "lint-staged": {
    "!(client)/**/*.{js,ts}": [
      "prettier --write"
    ]
  }
}
